{
  "name": "@jill64/svelte-storage",
  "description": "🗃️ Type-Safe Web Storage API Wrapper for Svelte",
  "version": "0.0.5",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-storage/issues",
  "homepage": "https://github.com/jill64/svelte-storage#readme",
  "files": [
    "dist",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde/index.d.ts",
      "default": "./dist/serde/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-storage.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "localstorage",
    "sessionstorage",
    "svelte",
    "type-safe"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx prettier --write . --plugin prettier-plugin-svelte .",
    "test": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/kit": "1.27.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-svelte": "2.35.1",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.8",
    "typescript": "5.3.2",
    "vite": "5.0.6"
  },
  "dependencies": {
    "@jill64/typed-storage": "2.0.5"
  }
}
